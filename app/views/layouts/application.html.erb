<!DOCTYPE html>
<html>
<head>
  <title>Endeavor</title>
  <%= stylesheet_link_tag 'application', 'token-input', media: 'all' %>
  <%= javascript_include_tag :defaults, 'jquery.tokeninput-mac' %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Cabin|Gudea|Raleway' rel='stylesheet' type='text/css'>
</head>
<body>

  <%- flash.each do |key, value| -%>
    <div class="flash flash-<%= key %>">
      <%= value %>
    </div>
  <%- end -%>
      <%= yield %>

  <%= javascript_include_tag 'application' %>
  <%= yield :extra_footer %>

</body>
</html>
